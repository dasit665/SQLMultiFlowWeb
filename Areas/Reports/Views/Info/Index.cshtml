@model List<SQLMultiFlowWeb.Areas.Reports.Models.InfoModel>
@{
    ViewData["Title"] = "Info table";
    Layout = "~/Areas/Reports/Views/Shared/_TableLayout.cshtml";
}

<main>
    <a asp-route="reports">к отчетам</a>
    <h3>Пользователь:@User.Identity.Name</h3>
    <h3>Info таблица</h3>
    <table>
        <tr>
            <th>ID:</th>
            <th>Дата и время:</th>
            <th>Сервер и БД:</th>
            <th>Скрипт и версия:</th>
            <th>Информацилнное сообщение:</th>
        </tr>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Id</td>
                <td>@i.DataTime</td>
                <td>@i.ServerDb</td>
                <td>@i.Script</td>
                <td>@i.MessageInfo</td>
            </tr>
        }
    </table>
</main>